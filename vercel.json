{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/news/:slug",
      "destination": "/api/news/:slug"
    },
    {
      "source": "/articles/:slug",
      "destination": "/api/articles/:slug"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}