{
  "name": "autamubilismo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run snap",
    "snap": "concurrently -k -s first \"serve -s dist -l 4567\" \"react-snap\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "reactSnap": {
    "source": "dist",
    "inlineCss": true,
    "skipThirdPartyRequests": true,
    "puppeteerArgs": [
      "--no-sandbox"
    ]
  },
  "dependencies": {
    "@portabletext/react": "^5.0.0",
    "@sanity/client": "^7.13.0",
    "@sanity/image-url": "^1.2.0",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/og": "^0.8.5",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "resend": "^6.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0",
    "serve": "^14.2.5",
    "tailwindcss": "^3.4.17",
    "vite": "^7.2.4"
  }
}
